{
  "name": "@vizlook/sdk",
  "version": "0.1.0",
  "description": "Vizlook JavaScript SDK",
  "author": "allen_wu",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "module": "./dist/index.mjs",
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "rm -rf dist && tsup src/index.ts --format cjs,esm",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\"",
    "release:patch": "./release.sh patch",
    "release:minor": "./release.sh minor",
    "release:major": "./release.sh major",
    "release:set-version": "./release.sh"
  },
  "keywords": [
    "vizlook",
    "video search",
    "AI",
    "RAG",
    "LLMs",
    "retrieval",
    "augmented",
    "generation"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vizlook/vizlook-js-sdk.git"
  },
  "homepage": "https://github.com/vizlook/vizlook-js-sdk#readme",
  "devDependencies": {
    "@types/node": "^24.3.0",
    "cross-env": "^10.0.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  }
}
